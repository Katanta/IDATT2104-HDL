#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x604c1374cf10 .scope module, "testbench_dot_product" "testbench_dot_product" 2 1;
 .timescale 0 0;
P_0x604c1374d0a0 .param/l "SIZE" 0 2 2, +C4<00000000000000000000000000000100>;
v0x604c1375fd90_0 .var "a", 31 0;
v0x604c1375fe50_0 .var "b", 31 0;
v0x604c1375ff20_0 .net "result", 15 0, v0x604c1375fc30_0;  1 drivers
S_0x604c1374d140 .scope module, "uut" "dot_product" 2 7, 3 1 0, S_0x604c1374cf10;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "a";
    .port_info 1 /INPUT 32 "b";
    .port_info 2 /OUTPUT 16 "result";
P_0x604c1374d400 .param/l "SIZE" 0 3 1, +C4<00000000000000000000000000000100>;
v0x604c137120f0_0 .net "a", 31 0, v0x604c1375fd90_0;  1 drivers
v0x604c1375fa90_0 .net "b", 31 0, v0x604c1375fe50_0;  1 drivers
v0x604c1375fb70_0 .var/i "i", 31 0;
v0x604c1375fc30_0 .var "result", 15 0;
E_0x604c1374b1e0 .event anyedge, v0x604c1375fc30_0, v0x604c137120f0_0, v0x604c1375fa90_0;
    .scope S_0x604c1374d140;
T_0 ;
    %wait E_0x604c1374b1e0;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x604c1375fc30_0, 0, 16;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x604c1375fb70_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x604c1375fb70_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x604c1375fc30_0;
    %load/vec4 v0x604c137120f0_0;
    %load/vec4 v0x604c1375fb70_0;
    %muli 8, 0, 32;
    %part/s 8;
    %pad/u 16;
    %load/vec4 v0x604c1375fa90_0;
    %load/vec4 v0x604c1375fb70_0;
    %muli 8, 0, 32;
    %part/s 8;
    %pad/u 16;
    %mul;
    %add;
    %store/vec4 v0x604c1375fc30_0, 0, 16;
    %load/vec4 v0x604c1375fb70_0;
    %addi 1, 0, 32;
    %store/vec4 v0x604c1375fb70_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x604c1374cf10;
T_1 ;
    %pushi/vec4 67305985, 0, 32;
    %store/vec4 v0x604c1375fd90_0, 0, 32;
    %pushi/vec4 134678021, 0, 32;
    %store/vec4 v0x604c1375fe50_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 19 "$display", "Dot product: %d", v0x604c1375ff20_0 {0 0 0};
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x604c1374cf10;
T_2 ;
    %vpi_call 2 25 "$dumpfile", "dump_dot_product.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x604c1374cf10 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test/testbench_dot_product.v";
